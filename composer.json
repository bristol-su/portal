{
    "name": "bristol-su/portal",
    "type": "project",
    "description": "Bristol SU Portal.",
    "keywords": [
    ],
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "aws/aws-sdk-php": "^3.88",
        "doctrine/dbal": "^2.10",
        "fideloper/proxy": "^4.0",
        "laracasts/utilities": "^3.0",
        "laravel/framework": "^8.0",
        "laravel/tinker": "^2.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "php-console/php-console": "^3.1",
        "bristol-su/support": "dev-portal-v4 as 4.7.2",
        "bristol-su/control": "dev-develop as 2.1.5",
        "fruitcake/laravel-cors": "^1.0",
        "ext-json": "*",
        "nao-pon/flysystem-google-drive": "~1.1",
        "laravel/socialite": "^5.0",
        "laravel/vapor-core": "^2.5",
        "predis/predis": "^1.1",
        "laravel/ui": "^3.0",
        "linkeys/signed-url": "^3.0.1",
        "twigger/laravel-translate": "dev-develop",
        "stichoza/google-translate-php": "^4.1",
        "bristol-su/portal-auth": "dev-master",
        "laravel/legacy-factories": "^v1.1.0"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "composer/composer": "^1.8",
        "filp/whoops": "^2.0",
        "arcanedev/log-viewer": "^8.0",
        "barryvdh/laravel-debugbar": "^3.4",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --force --tag=module"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/passport",
                "laravel/socialite",
                "bristol-su/control"
            ]
        }
    }
}
