{
    "name": "bristol-su/portal",
    "description": "Bristol SU Portal.",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "aws/aws-sdk-php": "^3.88",
        "doctrine/dbal": "^2.10",
        "fideloper/proxy": "^4.0",
        "laracasts/utilities": "^3.0",
        "laravel/framework": "^8.0",
        "laravel/tinker": "^2.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "php-console/php-console": "^3.1",
        "fruitcake/laravel-cors": "^1.0",
        "ext-json": "*",
        "nao-pon/flysystem-google-drive": "~1.1",
        "laravel/vapor-core": "^2.5",
        "predis/predis": "^1.1",
        "larabug/larabug": "^2.3",
        "bristol-su/control": "dev-develop as 3.1.0",
        "laravel/vapor-ui": "^1.0",
        "laravel/ui": "^3.0",
        "linkeys/signed-url": "^3.0.1",
        "twigger/laravel-translate": "dev-develop",
        "stichoza/google-translate-php": "^4.1",
        "laravel/legacy-factories": "^v1.1.0",
        "bristol-su/portal-auth": "^1.0.1",
        "bristol-su/support": "dev-develop as 5.1.0",
        "bristol-su/upload-file": "^3.0.3",
        "bristol-su/static-page": "^3.0.0",
        "bristol-su/typeform": "dev-develop as 3.0.1",
        "bristol-su/data-entry": "^3.0.0",
        "bristol-su/unioncloud-portal": "dev-develop as 2.0.0",
        "laravel/telescope": "^4.6",
        "bristol-su/assign-roles": "dev-develop as 3.0.0",
        "bristol-su/airtable": "^3.0.0"
    },
    "require-dev": {
        "itsgoingd/clockwork": "^5.0.3",
        "beyondcode/laravel-dump-server": "^1.0",
        "bmitch/churn-php": "^1.0",
        "composer/composer": "^1.8",
        "filp/whoops": "^2.0",
        "arcanedev/log-viewer": "^8.0",
        "barryvdh/laravel-debugbar": "^3.4",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3",
        "laravel/sail": "^1.2",
        "orchestra/testbench": "^6.21"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --force --tag=module",
            "@php artisan vapor-ui:publish --ansi",
            "@php artisan settings:keys",
            "@php artisan telescope:publish --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/passport",
                "laravel/socialite",
                "bristol-su/control"
            ]
        }
    },
    "abandoned": false
}
